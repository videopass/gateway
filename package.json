{
	"name": "videopass-gateway",
	"version": "0.0.1",
	"main": "lib/Gateway.js",
	"scripts": {
		"start": "concurrently 'npm run dev' 'npm run firestore'",
		"dev": "env-cmd -f .env nodemon --watch",
		"kill-emulator": "kill-port 8080 && kill-port 5001  && kill-port 5000",
		"firestore": "ttab npm run kill-emulator && firebase emulators:start --inspect-functions --only firestore",
		"test": "jest --watchAll --forceExit --detectOpenHandles",
		"test-impl": " env-cmd jest --watch functions",
		"update": "ncu -u @videopass*/* && npm install",
		"build": "rm -r ./lib && tsc && webpack",
		"deploy": "tsc && webpack",
		"install-service": "env-cmd -f .env node install.js",
		"clean": "depcheck ./"
	},
	"dependencies": {
		"@types/handbrake-js": "^5.0.1",
		"@videopass/ctms": "^0.0.8",
		"@videopass/ctms-core": "^0.0.2",
		"@videopass/ctms-model": "^0.0.7",
		"@videopass/firebase": "^0.0.12",
		"@videopass/model": "^0.0.44",
		"@videopass/services": "^0.0.31",
		"axios": "^1.4.0",
		"chokidar": "^3.5.3",
		"delay": "^5.0.0",
		"dotenv": "^16.0.3",
		"env-cmd": "^10.1.0",
		"fs-extra": "^11.1.1",
		"handbrake-js": "^7.0.0",
		"matcher": "^5.0.0",
		"moment": "^2.29.4",
		"rambda": "^7.5.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.1",
		"@types/jest": "^29.5.1",
		"@types/node": "^20.2.5",
		"concurrently": "^8.0.1",
		"jest": "^29.5.0",
		"jest-watch-typeahead": "^2.2.2",
		"kill-port": "^2.0.1",
		"node-windows": "^1.0.0-beta.8",
		"nodemon": "^2.0.22",
		"npm-check-updates": "^16.10.12",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"ttab": "^0.8.0",
		"typescript": "^5.0.4",
		"webpack": "^5.84.1",
		"webpack-cli": "^5.1.1",
		"webpack-node-externals": "^3.0.0"
	}
}
